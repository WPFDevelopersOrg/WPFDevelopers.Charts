<ws:Window x:Class="WPFDevelopers.Charts.Samples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ws="https://github.com/WPFDevelopersOrg.WPFDevelopers.Minimal"
        xmlns:wsCharts="https://github.com/WPFDevelopersOrg.WPFDevelopers.Charts"
        xmlns:local="clr-namespace:WPFDevelopers.Charts.Samples"
        mc:Ignorable="d"
        Title="WPFDevelopers.Charts" Height="450" Width="800">
    <Grid>
        <wsCharts:SectorChart  ItemsSource="{Binding ItemsSource,RelativeSource={RelativeSource AncestorType=local:MainWindow}}"
                                  Margin="30" />
    </Grid>
</ws:Window>
